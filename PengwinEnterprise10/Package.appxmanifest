                Executable="PengwinEnterprise10SW\PengwinEnterprise10SW.exe"
                EntryPoint="Windows.FullTrustApplication">
          <desktop:StartupTask
                  TaskId="pengwinenterprise10w-systemd"
                  Enabled="false"
                  DisplayName="Pengwin Enterprise 10 (systemd) - No Window"/>
        </desktop:Extension>
