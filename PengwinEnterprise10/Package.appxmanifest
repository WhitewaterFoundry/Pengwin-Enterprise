                Executable="Launcher10\PengwinEnterprise10.exe"
                EntryPoint="Windows.FullTrustApplication">
          <desktop:StartupTask
                  TaskId="pengwinenterprise10"
                  Enabled="false"
                  DisplayName="Pengwin Enterprise 10"/>
        </desktop:Extension>
      </Extensions>
    </Application>
